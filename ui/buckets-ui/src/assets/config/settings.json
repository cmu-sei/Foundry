        "filterProtocolClaims": true,
        "loadUserInfo": false,
        "monitorSession": false,
        "checkSessionInterval": 10000,
        "accessTokenExpiringNotificationTime": 120
      }
    },
    "corsPolicy": {
      "origins": [],
      "methods": [],
      "headers": [],
      "allowAnyOrigin": true,
      "allowAnyMethod": true,
      "allowAnyHeader": true,
      "supportsCredentials": false
    },
    "logging": {
      "includeScopes": false,
      "logLevel": {
        "default": "Warning"
      }
    },
    "urls": {
      "apiUrl": "http://localhost:5055/api"
    }
