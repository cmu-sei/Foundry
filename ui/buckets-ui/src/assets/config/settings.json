{
  "branding": {
      "applicationName": "Buckets",
      "identityLogoBaseUrl": "http://localhost:5000",
      "commit": "##COMMIT##"
    },
    "errorHandling": {
      "showDeveloperExceptions": false
    },
    "clientSettings": {
      "oidc": {
        "authority": "http://localhost:5000",
        "client_id": "stack-buckets",
        "redirect_uri": "http://localhost:5003/auth",
        "silent_redirect_uri": "http://localhost:5003/silent.html",
        "post_logout_redirect_uri": "http://localhost:5003",
