{
  "Branding": {
    "ApplicationName": "Foundry Buckets"
  },
  "ErrorHandling": {
    "ShowDeveloperExceptions": false
  },
  "ConnectionStrings": {
    "PostgreSQL": "Server=localhost;Database=buckets.db;Username=;Password=",
    "Sqlite": "Data Source=buckets.db",
    "SqlServer": "Server=(localdb)\\mssqllocaldb;Database=buckets;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "SecurityHeaders": {
    "ContentSecurity": "default-src * 'unsafe-inline' 'unsafe-eval' data: blob:",
    "XContentType": "nosniff",
    "XFrame": "SAMEORIGIN",
    "Cors": {
      "Origins": [],
      "Methods": [],
      "Headers": [],
      "AllowAnyOrigin": true,
      "AllowAnyMethod": true,
      "AllowAnyHeader": true,
      "SupportsCredentials": false
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error"
    }
  },
  "Database": {
    "AutoMigrate": true,
    "DevModeRecreate": false,
    "Provider": "PostgreSQL",
    "SeedTemplateKey": "cyber"
  },
  "Authorization": {
    "Authority": "http://localhost:5000",
    "AuthorizationUrl": "http://localhost:5000/connect/authorize",
    "AuthorizationScope": "",
    "ClientId": "",
    "ClientName": "Foundry Buckets",
    "ClientSecret": "",
    "RequireHttpsMetadata": false
  },
  "Console": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Options": {
    "Urls": {
      "SketchMarket": "http://localhost:5001/api"
    },
    "Storage": {
      "MaxFileBytes": 1E9,
      "StorageType": "Local"
    },
    "StorageLocal": {
      "Path": "files"
    },
    "StorageAzure": {
      "Key": "",
      "Name": "",
      "Container": ""
    }
  }
}
